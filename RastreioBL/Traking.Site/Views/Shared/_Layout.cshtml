<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Traking.Site</title>
    <!-- App css -->
    <link href="~/css/bootstrap-creative.min.css" rel="stylesheet" type="text/css" id="bs-default-stylesheet" />
    <link href="~/css/app-creative.min.css" rel="stylesheet" type="text/css" id="app-default-stylesheet" />

    <link href="~/css/bootstrap-creative-dark.min.css" rel="stylesheet" type="text/css" id="bs-dark-stylesheet" />
    <link href="~/css/app-creative-dark.min.css" rel="stylesheet" type="text/css" id="app-dark-stylesheet" />

    <!-- icons -->
    <link href="~/css/icons.min.css" rel="stylesheet" type="text/css" />

</head>
<body class="loading" data-layout-mode="horizontal" data-layout='{"mode": "light", "width": "fluid", "menuPosition": "fixed", "topbar": {"color": "dark"}, "showRightSidebarOnPageLoad": true}'>
    @RenderBody()

    @Html.Partial("footer")    \
    <!-- Vendor js -->
    <script src="~/js/vendor.min.js"></script>

    <!-- Plugins js-->
    <script src="~/lib/jquery-sparkline/jquery.sparkline.min.js"></script>
    <script src="~/lib/admin-resources/jquery.vectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/lib/admin-resources/jquery.vectormap/maps/jquery-jvectormap-world-mill-en.js"></script>

    <!-- Dashboard 2 init -->
    <script src="~/js/pages/dashboard-2.init.js"></script>

    <!-- App js-->
    <script src="~/js/app.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
